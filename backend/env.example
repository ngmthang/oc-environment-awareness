# Copy this to .env and fill in your values

# Get this from Neon.tech dashboard (your free PostgreSQL)
DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require

# Random string for session encryption - use any long random text
SESSION_SECRET=replace-with-a-long-random-secret-string

# Your frontend URL (for CORS) - e.g. https://yoursite.netlify.app
# Leave blank to allow all origins during development
FRONTEND_URL=

# Set to "production" when deploying
NODE_ENV=production
